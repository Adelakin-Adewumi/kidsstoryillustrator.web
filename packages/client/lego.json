    "MODULE_CSS": {
      "name": "<name>.module.css"
    },
    "MODULE_SCSS": {
      "name": "<name>.module.scss"
    },
    "REDUCER":{
      "name": "<name>.jsx",
      "template": "DEFAULT"
    },
    "ACTION":{
      "name": "<name>.jsx",
      "template": "DEFAULT"
    },
    "STORY":{
      "name": "<name>.story.jsx",
      "template": "./template/blockName.story.jsx"
